{
  "name": "sequential-async-foreach",
  "version": "1.0.3",
  "description": "An NPM package to properly handle using async functions in a forEach fashion (unlike Native forEach). Uses proper Promises and is light and efficient.",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && microbundle",
    "format-check": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest --coverage --passWithNoTests",
    "lint": "eslint ./src --ext .tsx,.ts",
    "type-check": "tsc --noEmit",
    "clean-packages": "rimraf node_modules && rimraf package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkershner6/sequential-async-foreach.git"
  },
  "keywords": [
    "await",
    "sequential",
    "async",
    "forEach",
    "Promise"
  ],
  "author": "Derek Kershner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkershner6/sequential-async-foreach/issues"
  },
  "homepage": "https://github.com/dkershner6/sequential-async-foreach#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "await-wait": "^1.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "microbundle": "^0.12.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {}
}
